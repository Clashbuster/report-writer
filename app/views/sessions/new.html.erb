
<!-- Side navigation -->
<div class="sidenav">
    <h1>Login</h1>
       
  </div>
  
  <!-- Page content -->
  <div class="main">
                
       <%= form_tag '/login' do %> <br />

       <%= label_tag :employee_id%> <br />
       <%= text_field_tag :employee_id %> <br />
 
       <%= label_tag :password%> <br />
       <%= password_field_tag :password%> <br />
 
       <%= submit_tag "Login"%>
    <%end%> <br />
 
    <p><form action="/signup" method="get">
       <input type="submit" value="New User" 
             name="Submit" id="frm1_submit" />
    </form></p>
 
        

</div>
                